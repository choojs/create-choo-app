{
  "name": "create-choo-app",
  "description": "Create a fresh choo application",
  "repository": "choojs/create-choo-app",
  "version": "1.6.0",
  "bin": {
    "create-choo-app": "./bin.js"
  },
  "scripts": {
    "deps": "dependency-check . && dependency-check . -i ./bin.js --extra --no-dev",
