    "pump": "^1.0.2",
    "rimraf": "^2.6.1"
  },
  "devDependencies": {
    "dependency-check": "~2.6.0",
    "standard": "^10.0.2"
  },
  "keywords": [
    "generate",
    "choo",
    "create"
  ],
  "license": "MIT"
}
